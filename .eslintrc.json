{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "amd": true,
        "jasmine": true 
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["exports"] }]
    }
}
