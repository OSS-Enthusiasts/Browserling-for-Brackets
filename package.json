{
  "name": "browserling-for-brackets",
  "version": "1.0.0",
  "description": "A developers-oriented toolset for many purposes (web, crypto, generators...)",
  "main": "main.js",
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "crypto-js": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jasmine": "^3.5.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/eslint ./src/main.js ./src/*  && ./node_modules/.bin/jasmine --random=false",
    "lintfix": "./node_modules/.bin/eslint --fix ./src/main.js ./src/* ./spec/*.js",
    "zip": "npm run-script build && ./node_modules/.bin/bestzip encode-decode.zip lib/* convertors/* main.js requirejs-config.json package.json",
    "build": "./node_modules/.bin/eslint ./src/* && ./node_modules/.bin/babel src -d ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OSS-Enthusiasts/Browserling-for-Brackets.git"
  },
  "keywords": [
    "toolset",
    "crypto",
    "web",
    "maths",
    "generators"
  ],
  "author": "OSSEnthusiasts",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OSS-Enthusiasts/Browserling-for-Brackets/issues"
  },
  "homepage": "https://github.com/OSS-Enthusiasts/Browserling-for-Brackets#readme"
}
